<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomPickerTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/primary</item>
        <item name="android:buttonBarButtonStyle">@style/CustomButtonStyle</item>
    </style>

    <style name="CustomButtonStyle" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="android:textColor">@color/primary</item>
    </style>

    <style name="MyRatingBar" parent="Widget.AppCompat.RatingBar">
        <!--non selected star color-->
        <item name="colorControlNormal">#4D000000</item>
        <!--selected star color-->
        <item name="colorControlActivated">@color/dialog_color_1</item>
    </style>

</resources>